    "eslint-loader": "2.1.1",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "node-sass-chokidar": "^1.3.0",
    "prop-types": "^15.6.1",
    "react-test-renderer": "^16.4.2"
  },
  "browserslist": [
